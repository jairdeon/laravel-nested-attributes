{
  "name": "thomisticus/laravel-nested-attributes",
  "type": "library",
  "description": "Nested attributes allow you to save attributes on associated records through the parent. By default nested attribute updating is turned off and you can enable it using the $nested attribute. When you enable nested attributes an attribute writer is defined on the model.",
  "keywords": [
    "Laravel",
    "Lumen",
    "Eloquent",
    "Nested Attributes"
  ],
  "version": "0.0.7",
  "homepage": "https://github.com/thomisticus/laravel-nested-attributes",
  "license": "MIT",
  "authors": [
    {
      "name": "Igor Moraes",
      "email": "igor.sgm@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/database": "^5.0|^6.0",
    "illuminate/support": "^5.0|^6.0",
    "mockery/mockery": "^0.9|^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Thomisticus\\NestedAttributes\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Thomisticus\\NestedAttributes\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --colors=always",
    "phpcs": "./vendor/bin/phpcs --standard=PSR12 --colors src/",
    "phpcbf": "./vendor/bin/phpcbf --standard=PSR12 --colors src/"
  },
  "config": {
    "sort-packages": true
  }
}
